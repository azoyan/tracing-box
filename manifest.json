{
    "name": "Tracing Paper App",
    "short_name": "Tracing Paper",
    "description": "Transform your device into a lightbox, allowing for tracing image directly from your device. Load image and place a paper atop the screen",
    "icons": [
        {
            "src": "/tracing-paper/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/tracing-paper/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "/tracing-paper/apple-touch-icon.png",
            "sizes": "180x180",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone",
    "scope": "/tracing-paper/",
    "start_url": "/tracing-paper/",
    "id": "/tracing-paper",
    "orientation": "portrait",
    "prefer_related_applications": true,
    "related_applications": [
        {
            "platform": "webapp",
            "url": "https://azoyan.github.io/tracing-paper/manifest.json",
            "id": "/tracing-paper"
        }
    ],
    "handle_links": "auto",
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "browser"
    ],
    "edge_side_panel": {
        "preferred_width": 480
    },
    "launch_handler": {
        "client_mode": "auto"
    },
    "dir": "ltr",
    "categories": [
        "photo",
        "productivity",
        "utilities",
        "artist",
        "draw",
        "plot",
        "design",
        "hobby"
    ],
    "lang": "en"
}